<html> 	
 
<script>
$(document).ready(function(){

	$("#addSelect").click(function(){

		$("#s1 option:selected").each(function(){
			$("#s2").append($(this));
		});
		
	}); 	 	 
		
	$("#delSelect").click(function(){

		$("#s2 option:selected").each(function(){
			$("#s1").append($(this));
		});
	});
	
	$("#result").click(function(){
		var s2_length = $("#s2 option").length;
		var result = "";
		var current_value = "";
		
		if(s2_length>0){
			for(i=0;i<s2_length;i++){
				current_value = $("#s2 option:eq("+i+")").val();
				if(i==0){
					result = current_value;										
				}else{
					result = result + "," + current_value;					
				}
			}
		}
	  	return result;
	});
	
});	 
</script>

<body>
<table>
	<tr>
		<td>
			<select multiple id="s1">
				<option>aaa</option>
				<option>bbb</option>
				<option>ccc</option>
				<option>ddd</option>
			</select>
		</td>
		<td>
			<input type="button" value="=>" id="addSelect"/><br>
			<input type="button" value="<=" id="delSelect"/><br>
			<input type="button" value="결과" id="result"/>			
		</td>
		<td>
			<select	 multiple id="s2"> 
			</select>
		</td>				
	</tr>
</table>


</body>
</html>