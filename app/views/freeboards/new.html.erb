<h1>New freeboard</h1>

<% form_for(@freeboard) do |f| %>
  <%= f.error_messages %>
<p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :content %><br />
    <%= f.text_area :content %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', freeboards_path %>